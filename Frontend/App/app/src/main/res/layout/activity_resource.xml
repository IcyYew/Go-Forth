<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResourceActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ResourceActivity"
        tools:layout_editor_absoluteX="-1dp"
        tools:layout_editor_absoluteY="-28dp">

        <TextView
            android:id="@+id/foodAmount"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:gravity="center"
            android:text="Food\nAmount:"
            android:textSize="24dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/FoodText" />

        <TextView
            android:id="@+id/WoodText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="11dp"
            android:layout_marginEnd="305dp"
            android:text="Wood\nAmount:"
            android:textSize="24dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/foodCollect" />

        <TextView
            android:id="@+id/woodCollect"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_marginStart="116dp"
            android:layout_marginTop="12dp"
            android:gravity="center"
            android:text="Wood\nAmount:"
            android:textSize="24dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/WoodText" />

        <TextView
            android:id="@+id/woodAmount"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="12dp"
            android:gravity="center"
            android:text="Wood\nAmount:"
            android:textSize="24dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/WoodText" />

        <TextView
            android:id="@+id/stoneCollect"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_marginStart="116dp"
            android:layout_marginTop="12dp"
            android:gravity="center"
            android:text="Stone\nAmount:"
            android:textSize="24dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/StoneText" />

        <TextView
            android:id="@+id/stoneAmount"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_marginStart="14dp"
            android:layout_marginTop="13dp"
            android:gravity="center"
            android:text="Stone\nAmount:"
            android:textSize="24dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/StoneText" />

        <TextView
            android:id="@+id/PlatinumText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="302dp"
            android:text="Platinum\nAmount:"
            android:textSize="24dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/stoneCollect" />

        <TextView
            android:id="@+id/platCollect"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_marginStart="116dp"
            android:layout_marginTop="8dp"
            android:gravity="center"
            android:text="Plat\nAmount:"
            android:textSize="24dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/PlatinumText" />

        <TextView
            android:id="@+id/platinumAmount"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="11dp"
            android:gravity="center"
            android:text="Plat\nAmount:"
            android:textSize="24dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/PlatinumText" />

        <TextView
            android:id="@+id/StoneText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="19dp"
            android:layout_marginEnd="305dp"
            android:text="Stone\nAmount:"
            android:textSize="24dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/woodCollect" />

        <TextView
            android:id="@+id/FoodText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="37dp"
            android:layout_marginEnd="304dp"
            android:text="Food\nAmount:"
            android:textSize="24dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Back" />

        <TextView
            android:id="@+id/foodCollect"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_marginStart="116dp"
            android:layout_marginTop="8dp"
            android:gravity="center"
            android:text="Food\nAmount:"
            android:textSize="24dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/FoodText" />

        <Button
            android:id="@+id/Back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="18dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="305dp"
            android:text="Back"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/platCollectButton"
            android:layout_width="192dp"
            android:layout_height="58dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="81dp"
            android:text="Collect"
            app:layout_constraintStart_toEndOf="@+id/platCollect"
            app:layout_constraintTop_toBottomOf="@+id/stoneCollectButton" />

        <Button
            android:id="@+id/stoneCollectButton"
            android:layout_width="194dp"
            android:layout_height="58dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="92dp"
            android:text="Collect"
            app:layout_constraintStart_toEndOf="@+id/stoneCollect"
            app:layout_constraintTop_toBottomOf="@+id/woodCollectButton" />

        <Button
            android:id="@+id/woodCollectButton"
            android:layout_width="197dp"
            android:layout_height="59dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="88dp"
            android:text="Collect"
            app:layout_constraintStart_toEndOf="@+id/woodCollect"
            app:layout_constraintTop_toBottomOf="@+id/foodCollectButton" />

        <Button
            android:id="@+id/foodCollectButton"
            android:layout_width="194dp"
            android:layout_height="57dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="176dp"
            android:text="Collect"
            app:layout_constraintStart_toEndOf="@+id/foodCollect"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>